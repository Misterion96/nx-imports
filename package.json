{
  "name": "repo",
  "version": "0.0.2",
  "scripts": {
    "-----hooks-----": "",
    "postinstall": "ngcc --properties es2020 browser module main",
    "-----cli-----": "",
    "ng": "ng",
    "nx": "nx",
    "lerna": "lerna",
    "commitlint": "commitlint",
    "-----utils-----": "",
    "nx:graph": "nx dep-graph",
    "-----affected-----": "",
    "affected:build": "yarn nx affected --target=build",
    "affected:lint": "yarn nx affected --target=lint",
    "affected:test": "yarn nx affected --target=test",
    "-----run-all-----": "",
    "run-all:build": "yarn nx run-many --target build",
    "-----ci-----": "",
    "ci:test:coverage": "nx run-many --target test --coverage --ci",
    "ci:test:cobertura:merge": "node scripts/cobertura-merge.js",
    "ci:i18n:extract": "yarn run nx extract-i18n i18n-static",
    "ci:i18n:create": "yarn run nx create-i18n i18n-static",
    "ci:app:checkout:build": "yarn run nx build app-checkout --prod",
    "ci:affected:build": "yarn nx affected --target=build --prod",
    "ci:affected:lint": "TIMING=30 yarn nx affected --target=lint --quiet --output-style static",
    "ci:affected:test": "yarn nx affected --target=test --ci --silent"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.0",
    "@angular/cdk": "^14.2.0",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/elements": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/material": "^14.2.0",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/router": "^14.2.0",
    "@ngneat/until-destroy": "^9.2.2",
    "@tinkoff/ng-polymorpheus": "4.1.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.11",
    "@angular-eslint/eslint-plugin": "~14.0.4",
    "@angular-eslint/eslint-plugin-template": "~14.0.4",
    "@angular-eslint/template-parser": "~14.0.4",
    "@angular/cli": "~14.2.11",
    "@angular/compiler-cli": "^14.2.0",
    "@angular/language-service": "~14.2.0",
    "@eslint/eslintrc": "2.0.2",
    "@nrwl/angular": "14.8.8",
    "@nrwl/cypress": "14.8.8",
