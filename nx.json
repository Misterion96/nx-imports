{
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "yarn.lock": "*",
    ".eslintrc.js": "*",
    "tsconfig.*.json": "*",
    "tsconfig.base.json": "*"
  },
  "affected": {
    "defaultBase": "origin/master"
  },
  "workspaceLayout": {
    "libsDir": "packages",
    "appsDir": "demos"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "test",
          "lint"
        ],
        "parallel": 3
      }
    }
  },
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "none"
    },
    "@nrwl/angular:library": {
      "linter": "eslint",
      "unitTestRunner": "jest",
      "buildable": true,
      "publishable": true,
      "compilationMode": "partial",
      "changeDetection": "OnPush",
      "prefix": "noda",
      "strict": true,
      "style": "scss"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [{ "projects": "dependencies", "target": "build" }]
    }
  },
  "defaultProject": "demo-additional-fields"
}
